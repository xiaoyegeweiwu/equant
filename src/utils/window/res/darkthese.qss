/* *****************************************标题栏相关*************************************/
/*标题栏*/
TitleBar {
    background-color: #3c3c3c;
}
#titleLabel  {
    background-color: #3c3c3c;
}
#iconLabel {
    background-color: #3c3c3c;
}
/*最小化最大化关闭按钮通用默认背景*/
#buttonMinimum,
#buttonMaximum,
#buttonClose {
    border: none;
    color: #646464;
    background-color: #3c3c3c;
}
/*悬停*/
#buttonMinimum:hover,#buttonMaximum:hover, #buttonClose:hover{
    background-color: #474748;
    color: white;
}
#buttonClose:hover {
    background-color:red;
}

/*鼠标按下不放*/
#buttonMinimum:pressed,#buttonMaximum:pressed {
    background-color: #474748;
}
#buttonClose:pressed {
    color: white;
    background-color: red;
}
#titleLabel {
    color: white;
}

/*自定义标题栏窗口边框颜色*/
#Framless {
    background: #363636;
}
